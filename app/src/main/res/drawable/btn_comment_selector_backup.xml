<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">


    <item android:drawable="@drawable/btn_background" android:state_pressed="true"></item>
    <item android:drawable="@drawable/btn_background" android:state_selected="true"></item>


    <item >
        <shape >
            <solid android:color="@color/white"></solid>
            <corners android:radius="3dp"></corners>

            <stroke android:color="#DEDFE0"
                android:width="1dp"/>
        </shape>
    </item>


</selector>
        <attr name="headtitle" format="string"></attr>
        <attr name="leftimg" format="integer"></attr>
        <attr name="rightimg" format="integer"></attr>
        <attr name="righttext" format="string"></attr>
        <attr name="lefttext" format="string"></attr>
